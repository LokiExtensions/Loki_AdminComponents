<?php
declare(strict_types=1);

/** @version 0.2.9 */

use Magento\Framework\Escaper;
use Magento\Framework\View\Element\Template;

/** @var Escaper $escaper */
/** @var Template $block */
$value = $block->getValue();

if (is_array($value)) {
    $value = 'array';
}


if (is_object($value)) {
    $value = 'object';
}
?>
<?php echo $escaper->escapeHtml($value) ?>
